title = {{Pulmonary function tests: High rate of false-negative results in the early detection and screening of scleroderma-related interstitial lung disease}},
url = {https://onlinelibrary.wiley.com/doi/full/10.1002/art.39405 https://onlinelibrary.wiley.com/doi/abs/10.1002/art.39405 https://onlinelibrary.wiley.com/doi/10.1002/art.39405},
