issn = {2296858X},
journal = {Frontiers in Medicine},
keywords = {SARS-CoV-2,Sequelae,follow-up,long-term COVID-19,multi-system assessment},
month = {nov},
pages = {2260},
publisher = {Frontiers Media SA},
title = {{Assessment of Sequelae of COVID-19 Nearly 1 Year After Diagnosis}},
volume = {8},
year = {2021}
}
